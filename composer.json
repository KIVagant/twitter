{
    "name": "pingpong/twitter",
    "description": "Simple Twitter API for Laravel 4",
    "keywords": ["twitter", "pingpong", "oauth", "laravel"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pingpong Labs",
            "email": "gue.warsono@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4|~5",
        "illuminate/session": "~4|~5",
        "illuminate/config": "~4|~5",
        "illuminate/routing": "~4|~5",
        "illuminate/http": "~4|~5",
        "guzzlehttp/guzzle": "4.*",
        "guzzlehttp/oauth-subscriber": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-0": {
            "Pingpong\\Twitter": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.3-dev"
        }
    },
    "minimum-stability": "dev"
}
